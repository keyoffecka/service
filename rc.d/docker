case "$command_name" in
  start)
    print_msg "Starting docker"
    
    nohup env PATH=$PATH:/opt/docker-1.13.1 /opt/docker-1.13.1/dockerd -H unix:///run/docker.sock --config-file /etc/docker/daemon.json >/var/log/docker.log 2>&1 &

    register_process_by_name dockerd
    success "$?"
    
    if [ "$ret_val" != "0" ] ; then
      echo "Failed to start Docker"
    fi
  ;;
esac
